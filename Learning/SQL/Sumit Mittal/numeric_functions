SELECT ABS(-10) AS _abs; -- 10
SELECT CEIL(5.1) AS ceil_ -- 6
SELECT FLOOR(5.1) AS ceil_ -- 5
SELECT ROUND(5.167,2) AS round_ -- 5.17
SELECT TRUNCATE(5.167,2) AS truncate_ -- 5.16
SELECT MOD(10,3) AS mod_ -- 1
SELECT POWER(10,3) AS power_ -- 1000
SELECT SQRT(16) AS SQRT_ -- 4
SELECT SIGN(-10) AS sign_ -- -1
SELECT RAND() AS rand_ -- 0.2845831003827817
